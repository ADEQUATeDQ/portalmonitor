{% extends "odpw_system_layout.jinja" %}
{% set title = "Fetch process elapsed time for the last ten weeks" -%}

{% set active_sub_page = "ui.systemfetch" -%}

{% block headcss %}
    {{ css_resources }}
{% endblock %}

{% block headscripts %}
    {{ js_resources }}
{% endblock %}


{% block systemcontent %}


            <div class="ui fourteen wide column centered" style="width: 20%; display: inline-block;" id="plot">
            {{ plot_div |indent(4)|safe}}
            </div>



    {{ plot_script }}
{% endblock %}


{% block script %}

$(document).ready(function() {
    var plot = Bokeh.Collections("Plot").models[0];
    plot.set("plot_width", $('#plot').width());
    plot.set("plot_height", $('#plot').height());
    console.log($('#plot').width())

});

{% endblock %}
