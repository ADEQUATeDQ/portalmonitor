<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN">
<html lang="en">
    <head>
    
        <title>Open Data Portal Watch</title>
        
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        
        {% block css %}
            <link rel="stylesheet" href="{{ static_url('css/kube.min.css') }}" />
            <link rel="stylesheet" href="{{ static_url('css/style.css') }}" />
            <link rel="stylesheet" href="{{ static_url('css/font-awesome.min.css')}}">
        {% endblock %}
        
        {% block scripts %}
            <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.js"></script>
            <script src="http://vega.github.io/vega-editor/vendor/d3.min.js"></script>
            <script src="http://vega.github.io/vega-editor/vendor/topojson.js"></script>
            <script src="http://vega.github.io/vega/vega.min.js"></script>
            
            <script type="text/javascript" src="{{ static_url('js/kube.min.js') }}" ></script>
            <script type="text/javascript" src="{{ static_url('js/jquery.dataTables.min.js')}}" ></script>
        {% endblock %}
    </head>
        
    <body>
        <div class="units-row">
            <div class="unit-100 unit-centered">
                <!-- Header -->
                {% include "_header.jinja" %}

                <!--  Navigation Bar -->
                {% include "_mainnav.jinja" %}

                <!-- Content -->
                <div id="main" class="units-row">
                    <aside class="unit-10">
                        <nav id="nav-side" class="nav">
                            <ul>
                                {% block sidenav %}
                    
                                {% endblock %}    
                            </ul>
                        </nav>
                    </aside>
                    
                    <section class="unit-90 unit-role-left">
                        {% block section %}
                    
                        {% endblock %}  
                    </section>
                </div>

                {% include "_footer.jinja" %}
                
            </div>
        </div>
        {% block pageScripts %}

        {% endblock %}
    </body>
</html>