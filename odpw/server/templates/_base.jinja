        {% block scripts %}
            
            <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
            <!--<script type="text/javascript" src="{{ static_url('js/plot.js') }}" ></script>-->
            <script type="text/javascript" src="{{ static_url('js/d3.v3.min.js') }}" ></script>
            <script type="text/javascript" src="{{ static_url('js/topojson.js') }}" ></script>
            <script type="text/javascript" src="{{ static_url('js/d3.geo.projection.v0.min.js') }}" ></script>
            <script type="text/javascript" src="{{ static_url('js/kube.min.js') }}" ></script>
            <script type="text/javascript" src="{{ static_url('js/vega.min.js') }}" ></script>
        {% endblock %}
    </head>
        
    <body>
        <!-- Header -->
        {% include "_header.jinja" %}
      
        <!--  Navigation Bar -->
        {% include "_mainnav.jinja" %}
       
        <!-- Content -->
        <div class="units-row container">
            {% block content %}{% endblock %}
        </div>
        
        {% include "_footer.jinja" %}
        {% block pageScripts %}
        
        {% endblock %}
    </body>
</html>