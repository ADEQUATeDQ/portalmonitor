<!{% extends "_base.jinja" %}

{% block scripts %}
{{ super() }}
            <script type="text/javascript" src="{{ static_url('js/list.min.js') }}" ></script>
            <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
{% endblock %}

{% block content %}
    <aside>
<nav class="side">
    <ul>
        <li>
            <a href="/"> <i class="fa fa-server fa-2x"></i>
                <p>Overview</p>
            </a>
        </li>
        <li>
            <a href="/"> <i class="fa fa-server fa-2x"></i>
                <p>Details</p>
            </a>
        </li>
        <li>
            <a href="/"> <i class="fa fa-server fa-2x"></i>
                <p>Quality</p>
            </a>
        </li>
        <li>
            <a href="/"> <i class="fa fa-server fa-2x"></i>
                <p>Activities</p>
            </a>
        </li>
        <li>
            <a href="/list/portals/" class="cur"> <i class="fa fa-cubes  fa-2x"></i>
                <p>Evolution</p>
            </a>
        </li>
    </ul>
</nav>
</aside>
<section class="content">
    <h2>Portal selection</h2>
    <div class="unit-100">
        <h3> Selection</h3>
    </div>
    
    <div class="units-row end">
        
        <div class="unit-20">
            <button class="btn singleportal"><i class="fa fa-cube"></i><br>Single portal</button>
        </div>
        <div class="unit-20">
            <a href="/portal_details/all" class="btn"><i class="fa fa-cubes"></i><br>All portals</a>
        </div>
        <div class="unit-20">
            <button class="btn"><i class="fa fa-filter"></i><br>Set of portals</button>
        </div>
    </div> 
    <div class="units-row">
        <div class="unit-push-20 unit-60">
            <form action="" class="forms">
                <div class="ui-widget">
                    <label for="tags">Portals: </label>
                    <input id="tags">
                </div>
            </form>
        </div>
        
    </div>
       
    
</section>
    
</div>
{% endblock %}

{% block pageScripts %}
    <script>
   $(function() {
    var availableTags = [
      "ActionScript",
      "AppleScript",
      "Asp",
      "BASIC",
      "C",
      "C++",
      "Clojure",
      "COBOL",
      "ColdFusion",
      "Erlang",
      "Fortran",
      "Groovy",
      "Haskell",
      "Java",
      "JavaScript",
      "Lisp",
      "Perl",
      "PHP",
      "Python",
      "Ruby",
      "Scala",
      "Scheme"
    ];
    $( "#tags" ).autocomplete({
      source: availableTags
    });
  });
    </script>
{% endblock %}